{
  "version": 1,
  "projects": {
    "gatsby-plugin": {
      "root": "packages/gatsby-plugin",
      "sourceRoot": "packages/gatsby-plugin/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/gatsby-plugin/**/*.ts",
              "packages/gatsby-plugin/**/*.spec.ts",
              "packages/gatsby-plugin/**/*.spec.tsx",
              "packages/gatsby-plugin/**/*.spec.js",
              "packages/gatsby-plugin/**/*.spec.jsx",
              "packages/gatsby-plugin/**/*.d.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "packages/gatsby-plugin/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/packages/gatsby-plugin",
            "tsConfig": "packages/gatsby-plugin/tsconfig.lib.json",
            "packageJson": "packages/gatsby-plugin/package.json",
            "main": "packages/gatsby-plugin/src/index.ts",
            "assets": [
              "packages/gatsby-plugin/README.md",
              "packages/gatsby-plugin/LICENSE",
              {
                "input": "./packages/gatsby-plugin/src",
                "glob": "**/.babelrc*",
                "output": "./src"
              },
              {
                "input": "./packages/gatsby-plugin/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./packages/gatsby-plugin/plugins",
                "glob": "**/*.!(ts)",
                "output": "./plugins"
              },
              {
                "input": "./packages/gatsby-plugin",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./packages/gatsby-plugin",
                "glob": "builders.json",
                "output": "."
              }
            ],
            "srcRootForCompilationRoot": "packages/gatsby-plugin"
          }
        },
        "publish": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "node tools/scripts/publish.js gatsby-plugin {args.version}"
          }
        }
      }
    },
    "gatsby-plugin-e2e": {
      "projectType": "application",
      "root": "packages/gatsby-plugin-e2e",
      "sourceRoot": "packages/gatsby-plugin-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "gatsby-plugin:build",
            "npmPackageName": "@nrwl/gatsby-plugin",
            "pluginOutputPath": "dist/packages/gatsby-plugin",
            "jestConfig": "packages/gatsby-plugin-e2e/jest.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  }
}
